  <DataSets>
    <DataSet Name="CustomerSales">
      <Query>
        <DataSourceName>SqlDatabase</DataSourceName>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>
          SELECT Customers.State,
          CONCAT(Customers.City, ', ', Customers.State) as City,
          Customers.Zipcode,
          Customers.Gender,
          SUM(InvoiceDetails.SalesAmount) As [Sales Revenue],
          SUM(InvoiceDetails.Quantity) As [Units Sold],
          COUNT(Customers.CustomerId) As [Customer Count]
          FROM
          Customers INNER JOIN
          Invoices ON Customers.CustomerId = Invoices.CustomerId INNER JOIN
          InvoiceDetails ON Invoices.InvoiceId = InvoiceDetails.InvoiceId
          GROUP BY
          Customers.State,
          Customers.City,
          Customers.Zipcode,
          Customers.Gender
        </CommandText>
      </Query>
      <Fields>
        <Field Name="State">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>State</DataField>
        </Field>
        <Field Name="City">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>City</DataField>
        </Field>
        <Field Name="Zipcode">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>Zipcode</DataField>
        </Field>
        <Field Name="Gender">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>Gender</DataField>
        </Field>
        <Field Name="Sales_Revenue">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>Sales Revenue</DataField>
        </Field>
        <Field Name="Units_Sold">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>Units Sold</DataField>
        </Field>
        <Field Name="Customer_Count">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>Customer Count</DataField>
        </Field>
      </Fields>
      <Filters>
        <Filter>
          <FilterExpression>=Fields!State.Value</FilterExpression>
          <Operator>In</Operator>
          <FilterValues>
            <FilterValue>=Parameters!State.Value</FilterValue>
          </FilterValues>
        </Filter>
      </Filters>
    </DataSet>
    <DataSet Name="StateList">
      <Query>
        <DataSourceName>SqlDatabase</DataSourceName>
        <rd:DesignerState>
          <QueryDefinition xmlns="http://schemas.microsoft.com/ReportingServices/QueryDefinition/Relational">
            <SelectedColumns>
              <ColumnExpression ColumnOwner="Customers" ColumnName="State" GroupBy="True" />
            </SelectedColumns>
          </QueryDefinition>
        </rd:DesignerState>
        <CommandText>SELECT
  Customers.[State]
FROM
  Customers
GROUP BY
  Customers.[State]
ORDER BY
  Customers.[State]</CommandText>
